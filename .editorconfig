# EditorConfig is a file format that helps developers define and maintain consistent coding styles between different editors and IDEs
# For more information about the EditorConfig file format, see https://editorconfig.org

# Top-most EditorConfig file
root = true

# Go files
[*.go]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
